{
  "name": "ollie",
  "version": "0.0.1",
  "description": "A website starter-kit for rapid SPA scaffolding",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node build/startMsg.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch",
    "open:src": "babel-node build/srcServer.js",
    "lint": "esw webpack.config.* src build --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "mocha --reporter progress build/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VirtuallyCreative/Ollie.git"
  },
  "keywords": [
    "SPA",
    "JS",
    "Framework",
    "Retto",
    "Ares"
  ],
  "author": "Al 'Vip3rousmango' Romano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VirtuallyCreative/Ollie/issues"
  },
  "homepage": "https://github.com/VirtuallyCreative/Ollie#readme",
  "dependencies": {
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/register": "7.0.0",
    "babel-loader": "8.0.4",
    "chai": "4.2.0",
    "chalk": "2.4.1",
    "cheerio": "1.0.0-rc.2",
    "compression": "1.7.3",
    "cross-env": "5.2.0",
    "css-loader": "1.0.1",
    "eslint": "5.8.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-watch": "4.0.2",
    "express": "4.16.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "3.2.0",
    "jsdom": "9.11.0",
    "json-schema-faker": "^0.4.7",
    "json-server": "0.12.2",
    "localtunnel": "1.9.1",
    "mocha": "5.2.0",
    "nock": "9.2.5",
    "npm-run-all": "4.1.3",
    "numeral": "2.0.6",
    "open": "0.0.5",
    "rimraf": "2.6.2",
    "style-loader": "0.23.1",
    "surge": "0.20.1",
    "webpack": "4.23.1",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.3",
    "webpack-md5-hash": "0.0.6"
  }
}
