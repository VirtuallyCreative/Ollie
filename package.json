{
  "name": "ollie",
  "version": "0.0.1",
  "description": "A website starter-kit for rapid SPA scaffolding",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node core/startMsg.js",
    "start": "npm-run-all --parallel open:src lint:watch start-mockapi test:watch",
    "open:src": "babel-node core/srcServer.js",
    "lint": "esw webpack.config.* src core --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "mocha --reporter progress core/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node core/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch api/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node core/build.js",
    "postbuild": "babel-node core/distServer.js",
    "deploy": "surge ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VirtuallyCreative/Ollie.git"
  },
  "keywords": [
    "SPA",
    "JS",
    "Framework",
    "Retto",
    "Ares"
  ],
  "author": "Al 'Vip3rousmango' Romano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VirtuallyCreative/Ollie/issues"
  },
  "homepage": "https://github.com/VirtuallyCreative/Ollie#readme",
  "dependencies": {
    "dotenv": "^8.0.0",
    "rollbar": "^2.7.1",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/register": "7.4.4",
    "babel-loader": "8.0.6",
    "chai": "4.2.0",
    "chai-http": "^4.3.0",
    "chalk": "2.4.2",
    "cheerio": "1.0.0-rc.3",
    "compression": "1.7.4",
    "cross-env": "5.2.0",
    "css-loader": "3.0.0",
    "eslint": "6.0.1",
    "eslint-plugin-import": "2.18.0",
    "eslint-watch": "5.1.2",
    "express": "4.17.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "faker": "^4.1.0",
    "html-webpack-plugin": "3.2.0",
    "jsdom": "^15.1.1",
    "json-schema-faker": "^0.5.0-rc17",
    "json-server": "0.15.0",
    "localtunnel": "^1.9.2",
    "mocha": "6.1.4",
    "npm-run-all": "4.1.5",
    "numeral": "2.0.6",
    "open": "6.4.0",
    "rimraf": "2.6.3",
    "style-loader": "0.23.1",
    "supertest": "^4.0.2",
    "surge": "^0.21.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "4.35.2",
    "webpack-dev-middleware": "3.7.0",
    "webpack-hot-middleware": "2.25.0",
    "webpack-md5-hash": "0.0.6"
  }
}
